{% block javascripts_head %}
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
{% endblock %}
{% block stylesheets_head %}
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
{% endblock %}
<nav class="navbar navbar-expand-lg bg-body-tertiary">
    <div class="container-fluid">
        <a class="navbar-brand" href="{{ path('app_notes') }}">loCode</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                <li class="nav-item">
                    <a class="nav-link {% if app.request.attributes.get('_route') == 'app_notes' %}active{% endif %}" aria-current="page" href="{{ path('app_notes') }}">Notes</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if app.request.attributes.get('_route') == 'app_json' %}active{% endif %}" href="{{ path('app_json') }}">JSON</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if app.request.attributes.get('_route') == 'app_hash' %}active{% endif %}" href="{{ path('app_hash') }}">Hash</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if app.request.attributes.get('_route') == 'app_base64' %}active{% endif %}" href="{{ path('app_base64') }}">BASE64</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if app.request.attributes.get('_route') == 'app_url' %}active{% endif %}" href="{{ path('app_url') }}">URL</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if app.request.attributes.get('_route') == 'app_token_generator' %}active{% endif %}" href="{{ path('app_token_generator') }}">Token</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if app.request.attributes.get('_route') == 'app_jwt' %}active{% endif %}" href="{{ path('app_jwt') }}">JWT</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if app.request.attributes.get('_route') == 'app_crontab' %}active{% endif %}" href="{{ path('app_crontab') }}">Crontab</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link {% if app.request.attributes.get('_route') == 'app_curl' %}active{% endif %}" href="{{ path('app_curl') }}">cURL</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle {% if app.request.attributes.get('_route') in ['app_php', 'app_php_serialize'] %}active{% endif %}" href="{{ path('app_php_serialize') }}" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        PHP
                    </a>
                    <ul class="dropdown-menu">
                        <li><a class="dropdown-item {% if app.request.attributes.get('_route') == 'app_php_serialize' %}active{% endif %}" href="{{ path('app_php_serialize') }}">Serialize</a></li>
                        <li><a class="dropdown-item {% if app.request.attributes.get('_route') == 'app_php_json' %}active{% endif %}" href="{{ path('app_php_json') }}">JSON</a></li>
                    </ul>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle {% if app.request.attributes.get('_route') in ['app_apache', 'app_apache_redirections'] %}active{% endif %}" href="{{ path('app_apache_redirections') }}" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        Apache
                    </a>
                    <ul class="dropdown-menu">
                        <li><a class="dropdown-item {% if app.request.attributes.get('_route') == 'app_apache_redirections' %}active{% endif %}" href="{{ path('app_apache_redirections') }}">Redirections</a></li>
                        <li><a class="dropdown-item {% if app.request.attributes.get('_route') == 'app_apache_vhost' %}active{% endif %}" href="{{ path('app_apache_vhost') }}">VirtualHost</a></li>
                    </ul>
                </li>
            </ul>
